name":null,"original":{"line":84,"column":0},"generated":{"line":100,"column":1}}],"sources":{"../../src/lib/ApiQuery.ts":"import { __DEV__ } from '../utils/constants'\nimport { isURIComponent } from '../utils/types'\n\ntype JsonApiQuery = {\n  [key: string]: JsonApiQueryParameter\n}\n\ntype JsonApiQueryParameter =\n  | JsonApiQueryParameterValue\n  | {\n      [key: string]: JsonApiQueryParameterValue\n    }\n\ntype JsonApiQueryParameterValue =\n  | string\n  | number\n  | {\n      [key: number]: JsonApiQueryParameterValue\n    }\n\nexport class ApiQuery {\n  [Symbol.species]: ApiQuery\n  value: Array<ApiQueryParameter>\n\n  constructor(query: JsonApiQuery) {\n    const parameters: Array<ApiQueryParameter> = []\n    Object.keys(query)\n      .sort()\n      .map((name: string) => {\n        const parameter = query[name]\n        if (typeof parameter === 'object') {\n          Object.keys(parameter).forEach((key) => {\n            const value = encodeQueryParameterValue((parameter as any)[key])\n            if (value.length) {\n              parameters.push(\n                new ApiQueryParameter(`${name}%5B${key}%5D`, value),\n              )\n            }\n          })\n        } else {\n          const value = encodeQueryParameterValue(parameter)\n          if (value.length) {\n            parameters.push(new ApiQueryParameter(name, value))\n          }\n        }\n      })\n    this.value = parameters\n  }\n\n  concat(query: ApiQuery | JsonApiQuery): ApiQuery {\n    // todo: sort concatenated query values\n    // todo: merge query values (duplicate parameter names)\n    if (ApiQuery.isApiQuery(query)) {\n      const result = new ApiQuery({})\n      result.value = this.value.concat(query.value)\n      return result\n    }\n    return this.concat(new ApiQuery(query))\n  }\n\n  toString(): string {\n    const parameters = this.value.join('&')\n    return parameters.length > 0 ? `?${parameters}` : ''\n  }\n\n  static isApiQuery(value: any): value is ApiQuery {\n    return value != null && value[Symbol.species] === ApiQuery\n  }\n}\n\nclass ApiQueryParameter {\n  name: string\n  value: string\n  constructor(name: string, value: string) {\n    this.name = isURIComponent.assert(name)\n    this.value = value\n  }\n\n  toString(): string {\n    return `${this.name}=${this.value}`\n  }\n}\n\nconst encodeQueryParameterValue = (value: JsonApiQueryParameterValue): string =>\n  Array.isArray(value)\n    ? value\n        .map(encodeURIComponent)\n        .filter(Boolean)\n        .sort()\n        .join(',')\n    : encodeURIComponent(value as string)\n"},"lineCount":null}},"error":null,"hash":"6490043d3ee9f7ed7a364ccf94a996c9","cacheData":{"env":{}}}