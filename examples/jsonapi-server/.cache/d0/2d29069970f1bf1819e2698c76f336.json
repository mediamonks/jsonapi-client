enerated":{"line":81,"column":0}},{"source":"../../src/utils/guard/lib/Type.ts","name":null,"original":{"line":57,"column":24},"generated":{"line":81,"column":7}},{"source":"../../src/utils/guard/lib/Type.ts","name":null,"original":{"line":57,"column":4},"generated":{"line":81,"column":8}},{"source":"../../src/utils/guard/lib/Type.ts","name":null,"original":{"line":58,"column":3},"generated":{"line":82,"column":0}}],"sources":{"../../src/utils/guard/lib/Type.ts":"import {\n  Description,\n  Static,\n  TypeGuard,\n  ErrorCode,\n  Label,\n} from '../utils/generics'\nimport { TypeValidationError } from './TypeValidationError'\n\nconst cloneTypeWith = <T extends Type<any>>(type: T, properties: {}): T =>\n  Object.assign(Object.create(Object.getPrototypeOf(type)), type, properties)\n\nexport class Type<T> {\n  code: ErrorCode = Symbol()\n  label: Label | null = null\n  description: Description\n  check: TypeGuard<T>\n  rules: Array<Type<T>>\n\n  constructor(description: Description, check: TypeGuard<T>) {\n    this.description = description\n    this.check = check\n    this.rules = [this]\n  }\n\n  validate(value: any): Array<TypeValidationError> {\n    const errors: Array<TypeValidationError> = []\n    for (let i = 0; i < this.rules.length; ++i) {\n      if (!this.rules[i].check(value)) {\n        errors.push(new TypeValidationError(this.rules[i]))\n      }\n    }\n    return errors\n  }\n\n  assert(value: any): Static<this> {\n    const errors = this.validate(value)\n    if (errors.length !== 0) {\n      throw errors\n    }\n    return value as Static<this>\n  }\n\n  toString(): string {\n    return this.description\n  }\n\n  withDescription(description: Description): this {\n    return cloneTypeWith(this, { description })\n  }\n\n  withLabel(label: Label): this {\n    return cloneTypeWith(this, { label })\n  }\n\n  withErrorCode(code: ErrorCode): this {\n    return cloneTypeWith(this, { code })\n  }\n}\n"},"lineCount":null}},"error":null,"hash":"bf2938a81701c644deb859