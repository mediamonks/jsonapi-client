 }\n\n  assert(\n    value: any,\n    description: Description = this.description || defaultValueLabel,\n  ): Static<this> {\n    for (let i = 0; i < this.rules.length; ++i) {\n      if (!this.check(value)) {\n        throw `${description} must be ${this.label}`\n      }\n    }\n    return value\n  }\n\n  toString(): Label {\n    return this.label\n  }\n\n  withLabel(label: string): this {\n    return cloneTypeWith(this, { label })\n  }\n\n  withDescription(description: string): this {\n    return cloneTypeWith(this, { description })\n  }\n}\n"},"lineCount":null}},"error":null,"hash":"e115ceaf62d377ab7dc9b2a6bd70f616","cacheData":{"env":{}}}