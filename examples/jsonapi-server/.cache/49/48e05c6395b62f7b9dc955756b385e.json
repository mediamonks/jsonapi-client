ics'\nimport { joinDescription } from '../util/joinDecription'\nimport { pushIfUnique } from '../util/pushIfUnique'\n\nexport class Type<T> {\n  description: Description\n  check: TypeGuard<T>\n  rules: Array<Type<T>>\n\n  constructor(description: Description, check: TypeGuard<T>) {\n    this.description = description\n    this.check = check\n    this.rules = [this]\n  }\n\n  validate(value: any): Array<Description> {\n    const errors: Array<Description> = []\n    return this.rules.reduce((result, type) => {\n      return type.check(value) ? result : result.concat(type.description)\n    }, errors)\n  }\n\n  assert(value: any, label: string = 'value'): Static<this> {\n    this.rules.forEach((type) => {\n      if (!type.check(value)) {\n        throw `${label} must be ${this.description}`\n      }\n    })\n    return value\n  }\n\n  toString(): Description {\n    return this.description\n  }\n}\n\nexport class And<T> extends Type<T> {\n  constructor(types: Array<Type<T>>) {\n    const andTypes: Array<any> = []\n    types.forEach((type) => {\n      if (type instanceof Or) {\n        pushIfUnique(andTypes, type)\n      } else {\n        type.rules.forEach((type) => pushIfUnique(andTypes, type))\n      }\n    })\n    super(\n      joinDescription(andTypes as any, 'and'),\n      (value: any): value is T => andTypes.every((type) => type.check(value)),\n    )\n    this.rules = andTypes\n  }\n}\n\nexport class Or<T> extends Type<T> {\n  constructor(types: Array<Type<T>>) {\n    const orTypes: Array<any> = []\n    types.forEach((type) => {\n      if (type instanceof And) {\n        pushIfUnique(orTypes, type)\n      } else {\n        type.rules.forEach((type) => pushIfUnique(orTypes, type))\n      }\n    })\n    super(\n      joinDescription(orTypes, 'or'),\n      (value: any): value is T => orTypes.some((type) => type.check(value)),\n    )\n    this.rules = orTypes\n  }\n}\n"},"lineCount":null}},"error":null,"hash":"5930744afd7e48eaebf49c01784959b4","cacheData":{"env":{}}}