: string | number | boolean | symbol | null | undefined,\n): string => (typeof value === 'string' ? `'${value}'` : String(value))\n\nexport const formatRegExp = (expression: RegExp): string => {\n  const [body] = String(expression).split(/\\/[g|i]*$/)\n  return [\n    body.length > 30 ? `${body.slice(0, 16)}...${body.slice(-8)}` : body,\n    expression.flags,\n  ].join('/')\n}\n\nexport const wrapPropertyKey = (key: PropertyKey) => `[${formatPrimitive(key)}]`\n\nexport const joinDescription = (\n  values: Array<string>,\n  conjugnation: string,\n): string => {\n  const length = values.length\n  switch (length) {\n    case 0:\n      return ''\n    case 1:\n      return String(values[0])\n    case 2:\n      return `${values[0]} ${conjugnation} ${values[1]}`\n  }\n  const lastIndex = length - 1\n  let result = `${values[0]}`\n  let index = 0\n  while (++index <= lastIndex) {\n    const value = values[index]\n    result += index === lastIndex ? `, ${conjugnation} ${value}` : `, ${value}`\n  }\n  return result\n}\n"},"lineCount":null}},"error":null,"hash":"98d6032d8b8dee8ef29130f6498f1eea","cacheData":{"env":{}}}