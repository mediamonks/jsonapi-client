:null,"original":{"line":118,"column":0},"generated":{"line":212,"column":1}}],"sources":{"../../src/lib/ApiEndpoint.ts":"import { __DEV__ } from '../utils/constants'\nimport { WithOptional, Nullable } from '../utils/generics'\nimport { Api } from './Api'\nimport { ApiController } from './ApiController'\nimport { ApiResourceQuery, ApiQuerySortOrder } from './ApiQuery'\nimport { ApiResult } from './ApiResult'\nimport {\n  AnyResource,\n  Resource,\n  ResourceFragment,\n  ResourceConstructor,\n  ResourceId,\n} from './Resource'\nimport { ResourceAttributeFieldNames } from './ResourceField'\nimport {\n  defaultResourceFilter,\n  ResourceFilter,\n  ResourceFilterOptions,\n} from './ResourceFilter'\nimport { ResourceIdentifierKey } from './ResourceIdentifier'\n\nexport type ApiEndpointPath = string\n\nexport type ResourceCreateData<R extends AnyResource> = WithOptional<\n  Resource<R>,\n  ResourceIdentifierKey\n>\n\nexport type ResourcePatchData<R extends AnyResource> = Partial<Resource<R>> & {\n  id: R['id']\n}\n\nexport class ApiEndpoint<R extends AnyResource> {\n  api: Api\n  path: ApiEndpointPath\n  Resource: ResourceConstructor<R>\n  pointers: Record<string, string>\n\n  constructor(\n    api: Api,\n    path: ApiEndpointPath,\n    Resource: ResourceConstructor<R>,\n  ) {\n    this.api = api\n    this.path = path\n    this.Resource = Resource\n    this.pointers = createResourcePointers(api, Resource)\n  }\n\n  sort(\n    name: ResourceAttributeFieldNames<R>,\n    ascending: boolean,\n  ): ApiQuerySortOrder<R> {\n    return new ApiQuerySortOrder(name, ascending)\n  }\n\n  ascend(name: ResourceAttributeFieldNames<R>): ApiQuerySortOrder<R> {\n    return this.sort(name, true)\n  }\n\n  descend(name: ResourceAttributeFieldNames<R>): ApiQuerySortOrder<R> {\n    return this.sort(name, false)\n  }\n\n  async get<F extends ResourceFilterOptions<R>>(\n    id: ResourceId,\n    filter: Nullable<ResourceFilter<R, F>> = null,\n  ): Promise<ApiResult<ResourceFragment<R, F>, any>> {\n    return ApiController.get(this.api).getResourceById(\n      this.Resource,\n      this.path,\n      id,\n      filter || defaultResourceFilter,\n    )\n  }\n\n  async query<F extends ResourceFilterOptions<R>>(\n    query: Nullable<ApiResourceQuery<R>> = null,\n    filter: Nullable<ResourceFilter<R, F>> = null,\n  ): Promise<ApiResult<Array<ResourceFragment<R, F>>, any>> {\n    return ApiController.get(this.api).getResourceCollection(\n      this.Resource,\n      this.path,\n      query || {},\n      filter || defaultResourceFilter,\n    )\n  }\n\n  async create(\n    data: ResourceCreateData<R>,\n  ): Promise<ApiResult<Resource<R>, any>> {\n    return ApiController.get(this.api).createResource(\n      this.Resource,\n      this.path,\n      data,\n    )\n  }\n\n  async patch(\n    data: ResourcePatchData<R>,\n  ): Promise<ApiResult<Resource<R>, any>> {\n    return ApiController.get(this.api).patchResource(\n      this.Resource,\n      this.path,\n      data,\n    )\n  }\n\n  async delete(id: ResourceId) {\n    throw new Error(`Endpoint#delete is not available as if now`)\n  }\n\n  toString(): string {\n    return this.path\n  }\n}\n\nconst createResourcePointers = <R extends AnyResource>(\n  api: Api,\n  Resource: ResourceConstructor<R>,\n): Record<string, string> => {\n  return Object.values(Resource.fields).reduce(\n    (pointers: any, field) => {\n      const pointerValue = [\n        'data',\n        field.root,\n        api.encodeFieldName(field.name),\n      ].join('/')\n      pointers[pointerValue] = field.name\n      return pointers\n    },\n    {\n      ['data/type']: 'type',\n      ['data/id']: 'id',\n    },\n  )\n}\n"},"lineCount":null}},"error":null,"hash":"056462af1ac0cd79c6daff14710c48d6","cacheData":{"env":{}}}